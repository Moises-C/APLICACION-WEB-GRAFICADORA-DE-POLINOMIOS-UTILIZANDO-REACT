<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>JSON</servlet-name>
        <servlet-class>administrador.JSON</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObtenerInfo</servlet-name>
        <servlet-class>administrador.ObtenerInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarUsuario</servlet-name>
        <servlet-class>administrador.EliminarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearUsuario</servlet-name>
        <servlet-class>administrador.CrearUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Insertar</servlet-name>
        <servlet-class>administrador.Insertar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerUsuario</servlet-name>
        <servlet-class>administrador.VerUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuario</servlet-name>
        <servlet-class>administrador.EditarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>administrador.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JSON</servlet-name>
        <url-pattern>/JSON</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObtenerInfo</servlet-name>
        <url-pattern>/ObtenerInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarUsuario</servlet-name>
        <url-pattern>/EliminarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearUsuario</servlet-name>
        <url-pattern>/CrearUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Insertar</servlet-name>
        <url-pattern>/Insertar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerUsuario</servlet-name>
        <url-pattern>/VerUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarUsuario</servlet-name>
        <url-pattern>/EditarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
